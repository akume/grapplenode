<h2>techniques</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Grapple.Models.Technique.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Guard</th>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//grapple/views/technique/list',{techniques: techniques})%>
	</tbody>
</table>
<h2>New technique</h2>
<form>
	<% for(var attr in Grapple.Models.Technique.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>